{
  "name": "Chrome Poster",
  "version": "1.1",
  "description": "Chrome poster is a developer tool that allows you to interact with http server",
  "update_url": "http://chrome-poster.googlecode.com/svn/trunk/updates.xml",
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "poster.png",
    "default_title": "Chrome Poster",
    "popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
